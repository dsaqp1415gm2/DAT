<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners
                android:radius="10dp"
                />
            <gradient
                android:startColor="@color/accent_material_dark"
                android:endColor="@color/accent_material_dark"
                android:angle="270"
                />
            <stroke
                android:dashGap="0dp"
                android:dashWidth="10dp"
                android:width="1dp"
                android:color="@color/accent_material_dark" />
        </shape>
    </item>
    <item>
        <shape>
            <corners
                android:radius="10dp"
                />
            <gradient
                android:startColor="@color/accent_color"
                android:endColor="@color/accent_color"
                android:angle="270"
                />
            <stroke
                android:dashGap="165dp"
                android:dashWidth="180dp"
                android:width="5dp"
                android:color="@color/accent_color3" />
        </shape>
    </item>
</selector>